{
    "presets": [
        [
        "@babel/preset-env",
            {
                "corejs": { "version": 3, "proposals": true },
                "modules": false,
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-amd",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
